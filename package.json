{
  "name": "automationqa",
  "version": "1.0.0",
  "description": "AQA courses (TMS)",
  "main": "index.js",
  "scripts": {
    "hw16": "ts-node ./hw16/index.ts",
    "hw17-test-mocha": "mocha --config hw17/mocharc.json",
    "hw17-test-jest": "jest --config hw17/jest.config.js",
    "hw18-api-tests": "jest --config hw18/api/jest.config.js",
    "hw19-selenium": "jest --config hw19/jest.config.js",
    "hw21-pageObjects": "mocha --config hw21/mocharc.json",
    "hw22-wdio": "wdio run hw22/config/wdio.conf.ts",
    "hw22-wdio-allure-report": "allure generate hw22/assets/allure-results --clean -o hw22/assets/allure-report && allure open hw22/assets/allure-report",
    "hw23-playwright": "playwright test --config hw23/configs/playwright.config.ts",
    "hw23-playwright-report": "playwright show-report hw23/assets/reports",
    "hw23-playwright-allure-report": "allure generate hw23/assets/allure-results --clean -o hw23/assets/allure-report && allure open hw23/assets/allure-report",
    "hw24-cy:open": "cypress open --config-file hw24/cy-e2e/cypress.config.ts",
    "hw24-cy:run": "cypress run --browser chrome --config-file hw24/cy-e2e/cypress.config.ts",
    "hw24-cy:allure-report": "allure generate hw24/cy-e2e/cypress/assets/allure-results --clean -o hw24/cy-e2e/cypress/assets/allure-report && allure open hw24/cy-e2e/cypress/assets/allure-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KatherineDubovik/AutomationQA.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KatherineDubovik/AutomationQA/issues"
  },
  "homepage": "https://github.com/KatherineDubovik/AutomationQA#readme",
  "devDependencies": {
    "@playwright/test": "^1.25.2",
    "@shelex/cypress-allure-plugin": "^2.31.0",
    "@types/chai": "^4.3.1",
    "@types/jest": "^28.1.6",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.6",
    "@types/selenium-webdriver": "^4.1.2",
    "@types/superagent": "^4.1.15",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@wdio/allure-reporter": "^7.23.0",
    "@wdio/cli": "^7.23.0",
    "@wdio/cucumber-framework": "^7.23.0",
    "@wdio/local-runner": "^7.23.0",
    "@wdio/spec-reporter": "^7.23.0",
    "allure-commandline": "^2.18.1",
    "allure-playwright": "^2.0.0-beta.19",
    "chai": "^4.3.6",
    "chromedriver": "^104.0.0",
    "cypress": "^10.8.0",
    "eslint": "^8.21.0",
    "jest": "^28.1.3",
    "log4js": "^6.6.1",
    "mocha": "^10.0.0",
    "selenium-webdriver": "^4.4.0",
    "superagent": "^8.0.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "wdio-chromedriver-service": "^7.3.2"
  }
}
